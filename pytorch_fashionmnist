   "source": [
    "from tqdm import tqdm\n",
    "from sklearn.metrics import accuracy_score\n",
    "epochs = 50\n",
    "\n",
    "\n",
    "for epoch in tqdm(range(epochs)):\n",
    "    train_loss = 0.0\n",
    "    test_loss = 0.0\n",
    "    \n",
    "    model.train()\n",
    "    for batch, (X_train, y_train) in enumerate(trainloader):\n",
    "        X_train, y_train = X_train.to(device), y_train.to(device)\n",
    "        y_hat = model(X_train).squeeze()\n",
    "        loss = criterion(y_hat, y_train)\n",
    "        train_loss += loss.item()\n",
    "        optimizer.zero_grad()\n",
    "        loss.backward()\n",
    "        optimizer.step()\n",
    "\n",
    "    model.eval()\n",
    "    pred=[]\n",
    "    true=[]\n",
    "    with torch.no_grad():\n",
    "        for batch, (X_test, y_test) in enumerate(testloader):\n",
    "            X_test, y_test = X_test.to(device), y_test.to(device)\n",
    "            y_pred = model(X_test).squeeze()\n",
    "            loss_test = criterion(y_pred, y_test)\n",
    "            test_loss += loss_test.item()\n",
    "            pred.extend(torch.argmax(y_pred, dim=1).detach().cpu().numpy())\n",
    "            true.extend(y_test.detach().cpu().numpy())\n",
    "            \n",
    "    test_acc = accuracy_score(true, pred)\n",
    "    \n",
    "    print(f\"Epoch: {epoch} | Train Loss: {train_loss} | Test Loss: {test_loss} | Test Acc = {test_acc}\")\n",
    "    scheduler.step(test_loss)"
   ]
  },
